// https://leetcode.com/problems/maximum-subarray/submissions/

class Solution {
public:
    int maxSubArray(vector<int>& nums) {
        int sum1 = nums[0];
        int maxSum = sum1;
        for(int i=1;i<nums.size();i++){
            sum1 = max(nums[i]+sum1, nums[i]);
            maxSum = max(maxSum, sum1);
        }
        
        return maxSum;
        
    }
};
